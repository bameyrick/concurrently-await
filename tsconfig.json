{
  "compilerOptions": {
    "outDir": "./dist",
    "moduleResolution": "node",
    "sourceMap": false,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "declaration": false,
    "target": "es2020",
    "module": "es2020",
    "lib": ["esnext", "DOM"]
  },
  "exclude": ["node_modules", "dist/"],
  "include": ["src/**/*"]
}
